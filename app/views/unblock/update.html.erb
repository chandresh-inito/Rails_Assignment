<%keytoupdate = params[:api_key]%>

<%@api_key = ApiKey.all%>

<%ispresent = 0%>

<%@api_key.each do |key|%>
    <%if key.api_key == keytoupdate then%>
        <%=key.api_key%>
        <%key.block_status="NO"%>
        <%key.save%>
        <%ispresent=1%>
        <%break%>
    <%end%>
<%end%>



<%if ispresent==1 then %>
    <%="Updated successfully" %>
<%elsif %>
    <%="Not Found "  %>
<%end %>

